import "std";

test "Bitwise" {
    std.assert(15 << 3 == 120, message: "<<");
    std.assert(15 >> 3 == 1, message: ">>");
    std.assert(15 ^ 3 == 12, message: "^");
    std.assert(15 \ 3 == 15, message: "\\");
    std.assert(~15 == -16, message: "~");
    std.assert(12 & 23 == 4, message: "&");
}