import "std";

fun main([str] args) > void {
    const m = (if (args.len() > 0) std.parseInt(args[0]) else null) ?? 3;
    const n = (if (args.len() > 1) std.parseInt(args[1]) else null) ?? 8;

    std.print("result: {ack(m, n)}");
}

fun ack(int m, int n) > int {
    if (m == 0) {
        return n + 1;
    }

    if (n == 0) {
        return ack(m: m - 1, n: 1);
    }

    return ack(
        m: m - 1,
        n: ack(m, n: n - 1)
    );
}